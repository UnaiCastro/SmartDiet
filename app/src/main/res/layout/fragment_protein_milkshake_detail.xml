<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0F192F">

    <TextView
        android:id="@+id/textView29"
        android:layout_width="149dp"
        android:layout_height="17dp"
        android:text="@string/protein_milkshake_recipe_title"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="50dp" />

    <RatingBar
        android:id="@+id/ratingBar3"
        android:layout_width="145dp"
        android:layout_height="47dp"
        android:background="#6EC7A7"
        android:progressTint="#FFEB3B"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="101dp" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/protein_milkshake_recipe_source"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="101dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="415dp"
        android:layout_height="292dp"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="187dp"
        tools:srcCompat="@drawable/protein_powder" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VER RECETA"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="446dp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/protein_milkshake_recipe_fact"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="519dp" />

</androidx.constraintlayout.widget.ConstraintLayout>